.team_data .field_wrap label {
    max-width: 210px;
    width: 100%;
    float: left;
    font-size: 15px;
    line-height: 18px;
    margin: 12px 0 0 0;
}

.team_data .field_wrap .field_wrap_inp {
    max-width: 425px;
    width: 100%;
    float: left;
    position: relative;
}

.team_data .field_wrap .field_wrap_inp:after {
    position: absolute;
    font-size: 15px;
    line-height: 18px;
    top: 13px;
    content: '*';
    right: 0;
}

.team_data .field_wrap .field_wrap_inp input {
    border: 1px solid #000000;
    font-size: 15px;
    line-height: 16px;
    color: #000;
    padding: 10px 14px;
    text-align: left;
    width: 97%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
}

.field_wrap {
    margin: 0 0 14px 0;
}

.field_wrap:after {
    clear: both;
}

.field_wrap:before, .field_wrap:after {
    content: "";
    display: table;
}

.team_data {
    width: 100%;
}

.team_data .field_wrap .field_wrap_inp textarea {
    font-size: 15px;
    line-height: 16px;
    color: #000000;
    padding: 10px 14px;
    border: 1px solid #000000;
    text-align: left;
    background: #fff;
    width: 97%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    resize: none;
}

.team_data .field_wrap .field_wrap_inp .field_wrap_inp_sel {
    border: 1px solid #5a5a5a;
    position: relative;
    margin: 0 0 0 0;
    width: 96%;
    height: 38px;
    display: inline-block;
    vertical-align: top;
}

.team_data .field_wrap .field_wrap_inp .field_wrap_inp_sel:before {
    content: '';
    right: 0;
    top: 0;
    width: 48px;
    height: 38px;
    background: #000;
    position: absolute;
    pointer-events: none;
    display: block;
}

.team_data .field_wrap .field_wrap_inp .field_wrap_inp_sel:after {
    font-family: FontAwesome;
    content: "\f107";
}

.team_data .field_wrap .field_wrap_inp .field_wrap_inp_sel:after {
    width: 48px;
    height: 38px;
    color: #fff;
    right: -16px;
    top: 0;
    padding: 0 0 0;
    position: absolute;
    pointer-events: none;
    line-height: 38px;
}

.team_data .field_wrap .field_wrap_inp .field_wrap_inp_sel select {
    margin: 0;
    background: #fff;
    color: #303030;
    font-size: 15px;
    line-height: 18px;
    outline: none;
    display: inline-block;
    -webkit-appearance: none;
    -moz-appearance: none;
    cursor: pointer;
    width: 100%;
    height: 38px;
    padding: 10px 12px;
    box-sizing: border-box;
    border-radius: 0;
}

.team_data .field_wrap .field_wrap_inp button {
    font-size: 17px;
    line-height: 18px;
    padding: 10px 34px;
    display: inline-block;
    color: #fff;
    -webkit-appearance: none;
    float: left;
    transition: all 0.2s ease-out;
}

.team_data .field_wrap .field_wrap_inp span {
    float: right;
    font-size: 15px;
    line-height: 18px;
    margin: 11px 12px 0 0;
}

