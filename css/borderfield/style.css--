.outline-block {
    display: block;
    width: 100%;
    height: 1px;
    clear: both;
}

.center-block {
    margin: auto;
}

.fancybox-nav span {
    visibility: visible;
}

.fancybox-nav .button-next, .fancybox-nav .button-prev {
    display: block;
    height: 54px;
    width: 54px;
}

.fancybox-nav .button-next {
    margin-right: -100px;
    background: url(/images/carousel_next.png) repeat left top;
    visibility: visible;
}

.fancybox-nav .button-prev {
    margin-left: -100px;
    background: url(/images/carousel_prev.png) repeat left top;
    visibility: visible;
}

.fancybox-title-float-wrap {
    left: 0;
    right: 0;
}

.fancybox-opened .fancybox-title {
    visibility: visible;
    position: absolute;
    top: 80%;
    margin: auto;
    background: url(/images/watermark_logo.png) no-repeat left top;
    width: 100%;
}

.fancybox-opened .fancybox-title .child {
    display: none;
}

.fancybox-opened .fancybox-skin { box-shadow: none; padding: 0; }

.loader {
    margin: 0 auto;
    width: 100px;
}

.circular {
    animation: rotate 2s linear infinite;
    height: 100%;
    transform-origin: center center;
    width: 100%;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}

.path {
    stroke-dasharray: 1, 200;
    stroke-dashoffset: 0;
    animation: dash 1.5s ease-in-out infinite, color 6s ease-in-out infinite;
    stroke-linecap: round;
}

.static-block.intro {
    height: 600px;
}

.social-networks ul li {
    border: none;
}

@keyframes rotate {
    100% {
        transform: rotate(360deg);
    }
}

@keyframes dash {
    0% {
        stroke-dasharray: 1, 200;
        stroke-dashoffset: 0;
    }
    50% {
        stroke-dasharray: 89, 200;
        stroke-dashoffset: -35px;
    }
    100% {
        stroke-dasharray: 89, 200;
        stroke-dashoffset: -124px;
    }
}

@keyframes color {
    100%,
    0% {
        stroke: #e15352;
    }
    40% {
        stroke: #e15352;
    }
    66% {
        stroke: #e15352;
    }
    80%,
    90% {
        stroke: #e15352;
    }
}

