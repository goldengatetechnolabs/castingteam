@font-face {
    font-family: 'American Typewriter Regular';
    src: url('/fonts/American Typewriter Regular.ttf');
}

h1, h2, p {
    margin: 0;
    padding: 0;
}

a {
    text-decoration: none;
    color: inherit;
    cursor: pointer;
}

ul {
    text-decoration: none;
    list-style-type: none;
    padding: 0;
    margin: 0;
}
button, select {
    border: none;
}

#loading, #no-loading {
    display: none;
    text-align: center;
    margin: auto;
    padding: 5px;
    height: 150px;
    background-color: transparent;
}

.overview #loading {
    display: block;
}

.dsp_none {
    display: none;
}

.loading {
    position: fixed;
    z-index: 200;
    bottom: 0;
    left: 0;
    width: 100%;
    text-align: center;
    margin: 0 0 95px 0;
}

#modal-bg {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 500;
    background-color: white;
    width: 100%;
    height: 100%;
    zoom: 1;
    filter: alpha(opacity=50);
    opacity: 0.5;
}

#modal.alert {
    padding: 20px;
    width: 250px;
    text-align: center;
    position: fixed;
    margin: auto;
    top: 200px;
}

#modal {
    z-index: 600;
    left: 45%;
    background-color: #ffffff;
    border: 1px solid #C2C2C2;
    -webkit-box-shadow: 0 3px 17px #CDCDCD;
    -moz-box-shadow: 0 3px 17px #CDCDCD;
    box-shadow: 0 3px 17px #CDCDCD;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
}

#modal a {
    background: #000 none repeat scroll 0 0;
    color: #fff;
    display: block;
    font-size: 17px;
    line-height: 17px;
    padding: 10px 0;
    position: relative;
    text-align: center;
    transition: all 0.2s ease-out 0s;
    width: 120px;
    margin: auto;
    margin-top: 20px;
}

#no-loading span {
    display: block;
    margin: 10px;
}

.underlined_text {
    text-decoration: underline;
}

.fl_lft {
    float: left;
}

.ps_ab {
    position: absolute;
}

.ps_ab .menu_right {
    margin-top: 30px;
    margin-right: 40px;
}

.common-header .menu_right ul li:last-child {
    padding-right: 0;
}

.slick-arrow {
    z-index: 2;
}

.slick-image-section .slick-prev {
    left: 24px;
    background: url(/images/carousel_prev_white.png) repeat left top;
}

.slick-image-section .slick-prev:before {
    content: '';
}

.slick-image-section .slick-next {
    display: inline-block;
    background: url(/images/carousel_next_white.png) repeat left top;
    right: 24px;
}

.slick-image-section .slick-next:before {
    content: '';
}

.slick-image-section button {
    color: transparent;
    position: absolute;
    width: 55px;
    height: 55px;
    top: 50%;
}

.header-slider-block .image-section {
    overflow: hidden;
}

.header-slider-block .image-section .slick-arrow {
    display: none !important;
}

.image-section img {
    display: inline;
}

.homepage-slide-wrapper, .latest-models {
    position: relative;
    clear: both;
}

header {
    width: 100%;
    clear: none;
    z-index: 100;
    font-size: 24px;
    height: 100px;
}

header .menu_right li:before {
    background: #ffffff none repeat scroll 0 0;
    content: "";
    height: 20px;
    left: 0;
    position: absolute;
    top: 46%;
    transform: translateY(-50%);
    width: 1px;
}

header .red-color.menu_right li:before {
    background: #e15352 none repeat scroll 0 0;
}

header .menu_right li:first-child:before {
    background: none;
}

header .logo {
    width: 130px;
    margin-left: -25px;
}

section {
    width: 100%;
}

section .block-title {
    padding: 90px 0;
    width: 1200px;
    margin: auto;
}

section h2{
    font-size: 68px;
    font-weight: normal;
    margin-bottom: 20px;
}

body {
    font-family: 'American Typewriter Regular', sans-serif;
    margin: 0;
    overflow-x: hidden;
}

.body-wrapper {
    border: 18px solid white;
}

.slide-up-menu {
