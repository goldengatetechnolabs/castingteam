    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
}

.label1:before {
    content: '';
    right: 0;
    top: 0;
    width: 42px;
    height: 38px;
    background: #e15352;
    position: absolute;
    pointer-events: none;
    display: block;
}

.label1:after {
    content: '';
    background: url(../images/big_drop_icon.png) no-repeat center center;
    width: 42px;
    height: 38px;
    color: #FFFFFF;
    right: 0;
    top: 0;
    padding: 0 0 0;
    position: absolute;
    pointer-events: none;
}

