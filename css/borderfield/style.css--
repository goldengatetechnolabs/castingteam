.label1 {
    position: relative;
    margin: 16px 0 0 0;
    border: 3px solid #e15352;
    width: 349px;
    height: 38px;
    display: inline-block;
    vertical-align: top;
}

.label1 select {
    margin: 0;
    background: #ffffff;
    color: #000000;
    font-size: 17px;
    line-height: 18px;
    border: none;
    outline: none;
    display: inline-block;
    -webkit-appearance: none;
    -moz-appearance: none;
    cursor: pointer;
    width: 349px;
    height: 38px;
    padding: 7px 20px;
